[[ -f ~/.bashrc ]] && . ~/.bashrc

# Variaveis do clipmenu, clipboard baseado no dmenu
# Para documentacao: clipmenud -h, clipmenu -h, clipctl -h.
export CM_SELECTIONS="clipboard"
export CM_OUTPUT_CLIP=0
export CM_MAX_CLIPS=64

